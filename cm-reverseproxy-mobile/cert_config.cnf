[ req ]
default_bits       = 4096
default_md         = sha256
prompt             = no
distinguished_name = dn
req_extensions     = req_ext

[ dn ]
C  = CO
ST = Bogota D.C
L  = Bogota
O  = CuentasClaras
OU = CuentasClaras
CN = localhost

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = cuentasclaras.com
DNS.2 = www.cuentasclaras.com